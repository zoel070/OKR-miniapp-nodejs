.objective-created-btn {
  height: 40px;
  line-height:40px;
  background: rgb(240, 170, 40);
  color:  #fff;
  font-size: 18px;
  text-align: center;
  border-radius: 10px;
  width: 160px;
  margin: auto auto;
  margin-bottom: 20px;
  font-weight: 600;
}

.objective-item {
  position: relative;
  background: #178d96;
  padding: 10px;
  margin-bottom: 5px;
  overflow: hidden;
}

.objective-item.finished:after {
  position: absolute;
  top: -20px;
  right: -20px;
  width: 80px;
  height: 80px;
  border: 8px solid red;
  content: '完成';
  line-height: 78px;
  text-align: center;
  font-size: 22px;
  font-weight: 600;
  color: red;
  border-radius: 50%;
  opacity: .7;
  transform: rotate(-45deg);
}

.objective-title {
  font-size: 14px;
  line-height: 20px;
  color: #fff;
  margin-bottom: 4px;
}

.objective-created_time {
  font-size: 12px;
  color: #fff;
}